{
  "name": "Chrome Extension with React & Webpack",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "idle",
    "webNavigation",
    "*://*/*"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "css": ["content.styles.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "content.styles.css",
    "icon-128.png",
    "icon-34.png"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src script-src-elem 'self' 'unsafe-inline'; object-src 'self'"
}
